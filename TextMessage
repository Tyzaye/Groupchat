/**
 * 
 */

/**
 * @author tyste
 *
 */
public class TextMessage {
	// TextMessage class
	private String text;
	private Member member;
	private int time;

	/**
	 * @param t
	 * @param m
	 * @param time
	 */
	public TextMessage(String t, Member m, int ti) {
		text = t;
		member = m;
		time = ti;
	}

// getters and setters
	/**
	 * @return
	 */
	public String getText() {
		return text;
	}

	/**
	 * @param text
	 */
	public void setText(String text) {
		this.text = text;
	}

	/**
	 * @return
	 */
	public Member getMember() {
		return member;
	}

	/**
	 * @param member
	 */
	public void setMember(Member member) {
		this.member = member;
	}

	/**
	 * @return
	 */
	public int getTime() {
		return time;
	}

	/**
	 * @param time
	 */
	public void setTime(int time) {
		this.time = time;
	}

}
