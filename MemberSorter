import java.util.*;

/**
 * @author tyste
 *
 */
@SuppressWarnings("rawtypes")
public class MemberSorter implements Iterable {
	private ArrayList<Member> member;

	/**
	 * 
	 */
	public MemberSorter() {
		this.member = new ArrayList<Member>();
	}

	/**
	 * @param member
	 */
	public MemberSorter(ArrayList<Member> member) {
		this.member = member;
	}

	/**
	 * 
	 */
	public void sortByTime() {
		Collections.sort(member);
	}

	/**
	 *
	 */
	@Override
	public String toString() {
		return "List of Groupchat Members: [" + member + "]";
	}

	/**
	 *
	 */
	@Override
	public Iterator iterator() {
		return null;
	}
}
